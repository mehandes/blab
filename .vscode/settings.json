{
    "java.project.sourcePaths": [
        "types/java/src/main/java",
        "types/java/src/test/java",
        "commons/java/src/main/java",
        "commons/java/src/test/java",
        "blender/river/core/src/main/java",
    ],
    "java.project.referencedLibraries": [
        "lib/**/*.jar",
        "bazel-bin/external/**/*.jar"
    ],
    "java.format.settings.url": ".vscode/java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "[java]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "redhat.java",
        "editor.tabSize": 2,
    },
    "[c]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.tabSize": 2
    },
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.clang_format_style": "file"
}